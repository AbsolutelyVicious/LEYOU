<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.com.leyou.service.core.dao.SkuDAO">

    <select id="selectSkuList" resultType="Sku" parameterType="Sku">
        SELECT
            *
        FROM
            bbs_sku
        WHERE
            product_id = #{value}
    </select>

</mapper>